[57.613296, "o", "\r\n\u001b[1m\u001b[0;36mSTEP 7: USING PATTERNS FOR STANDARDIZED SUBNET CREATION\u001b[0m\r\n"]
[58.121843, "o", "\r\n\u001b[1m\u001b[0;34m## Creating reusable subnet pattern for web tier\u001b[0m\r\n"]
[58.633434, "o", "\u001b[0;33m$ \u001b[1m./ipam pattern create --name web-subnet --cidr-size 24 --environment prod --region us-east1 --block 10.0.0.0/16 --file prod\u001b[0m\r\n"]
[59.153342, "o", "Pattern created successfully!\r\n"]
[60.163627, "o", "\r\n\u001b[1m\u001b[0;34m## Listing available patterns\u001b[0m\r\n"]
