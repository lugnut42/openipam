[5.20653, "o", "\r\n\u001b[0;35mCleaning previous environment...\u001b[0m\r\n"]
[6.223917, "o", "\r\n\u001b[1m\u001b[0;34m## Step 1: Building OpenIPAM...\u001b[0m\r\n"]
[6.73457, "o", "\u001b[0;33m$ \u001b[1mmake clean && make build\u001b[0m\r\n"]
[7.261428, "o", "rm -f ipam validate-blocks coverage.out\r\n"]
[7.274212, "o", "go build -o ipam -v\r\n"]
[7.536552, "o", "github.com/lugnut42/openipam\r\n"]
[7.88852, "o", "go build -o validate-blocks scripts/validate_blocks.go\r\n"]
[9.021877, "o", "\r\n\u001b[1m\u001b[0;34m## Checking current state\u001b[0m\r\n"]
[9.53258, "o", "\u001b[0;33m$ \u001b[1mls -la ~/.openipam/blocks 2>/dev/null || echo 'No block files exist yet - clean state'\u001b[0m\r\n"]
[10.047852, "o", "No block files exist yet - clean state\r\n"]
[11.057876, "o", "\r\n\u001b[1m\u001b[0;36mSTEP 2: SETTING UP DEVELOPMENT ENVIRONMENT\u001b[0m\r\n"]
[11.569022, "o", "\r\n\u001b[1m\u001b[0;34m## Setting configuration path\u001b[0m\r\n"]
[12.578529, "o", "\u001b[0;33m$ \u001b[1mexport IPAM_CONFIG_PATH=$HOME/.openipam\u001b[0m\r\n"]
[13.59112, "o", "Environment variable IPAM_CONFIG_PATH set to /Users/t/.openipam\r\n"]
[15.608677, "o", "\r\n\u001b[1m\u001b[0;34m## Initializing configuration with dev block file\u001b[0m\r\n"]
[16.118296, "o", "\u001b[0;33m$ \u001b[1m./ipam config init dev\u001b[0m\r\n"]
[16.858013, "o", "Configuration initialized successfully:\r\n  Config file: /Users/t/.openipam/ipam-config.yaml\r\n  Block file: /Users/t/.openipam/blocks/dev.yaml (dev)\r\n"]
[17.868168, "o", "\r\n\u001b[1m\u001b[0;36mSTEP 3: CREATING DEVELOPMENT NETWORK BLOCK\u001b[0m\r\n"]
[18.380274, "o", "\r\n\u001b[1m\u001b[0;34m## Creating development block (192.168.0.0/16)\u001b[0m\r\n"]
[18.891297, "o", "\u001b[0;33m$ \u001b[1m./ipam block create --cidr 192.168.0.0/16 --description 'Development Network' --file dev\u001b[0m\r\n"]
[19.409511, "o", "Created block 192.168.0.0/16 in dev file\r\n"]
[20.419193, "o", "\r\n\u001b[1m\u001b[0;34m## Showing development block details\u001b[0m\r\n"]
[20.93102, "o", "\u001b[0;33m$ \u001b[1m./ipam block show 192.168.0.0/16 --file dev\u001b[0m\r\n"]
[21.451621, "o", "Block CIDR      Description\r\n192.168.0.0/16  Development Network\r\n\r\nSubnets:\r\nSubnet CIDR  Name  Region\r\n"]
[22.461751, "o", "\r\n\u001b[1m\u001b[0;34m## Viewing available space in development block\u001b[0m\r\n"]
[22.972643, "o", "\u001b[0;33m$ \u001b[1m./ipam block available 192.168.0.0/16 --file dev\u001b[0m\r\n"]
[23.490239, "o", "Available CIDR Ranges\r\n192.168.0.0/16\r\n"]
[24.500332, "o", "\r\n\u001b[1m\u001b[0;36mSTEP 4: MANAGING SUBNETS IN DEVELOPMENT\u001b[0m\r\n"]
[25.009398, "o", "\r\n\u001b[1m\u001b[0;34m## Creating web tier subnet\u001b[0m\r\n"]
[25.518262, "o", "\u001b[0;33m$ \u001b[1m./ipam subnet create --block 192.168.0.0/16 --cidr 192.168.1.0/24 --name 'web-tier' --region us-west1\u001b[0m\r\n"]
[26.039562, "o", "Subnet created successfully!\r\n"]
[27.04981, "o", "\r\n\u001b[1m\u001b[0;34m## Creating application tier subnet\u001b[0m\r\n"]
[27.561443, "o", "\u001b[0;33m$ \u001b[1m./ipam subnet create --block 192.168.0.0/16 --cidr 192.168.2.0/24 --name 'app-tier' --region us-west1\u001b[0m\r\n"]
[28.08105, "o", "Subnet created successfully!\r\n"]
[29.091123, "o", "\r\n\u001b[1m\u001b[0;34m## Creating database tier subnet\u001b[0m\r\n"]
[29.602037, "o", "\u001b[0;33m$ \u001b[1m./ipam subnet create --block 192.168.0.0/16 --cidr 192.168.3.0/24 --name 'db-tier' --region us-west1\u001b[0m\r\n"]
[30.121179, "o", "Subnet created successfully!\r\n"]
